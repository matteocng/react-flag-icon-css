{
  "name": "react-flag-icon-css",
  "description": "React SVG country flags component",
  "homepage": "https://github.com/matteocng/react-flag-icon-css#readme",
  "version": "1.0.7",
  "main": "./lib",
  "author": {
    "email": "matteocng+npmjs@outlook.com",
    "name": "matteocng",
    "url": "https://github.com/matteocng"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteocng/react-flag-icon-css.git"
  },
  "bugs": {
    "url": "https://github.com/matteocng/react-flag-icon-css/issues"
  },
  "keywords": [
    "react",
    "flag-icon",
    "flag-icons",
    "flag-icon-css",
    "react-css-modules"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "flag-icon-css": "^2.8.0",
    "react-css-modules": "^4.1.0",
    "tcomb": "^3.2.16",
    "tcomb-react": "^0.9.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "classnames": "^2.2.5",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "flag-icon-css": "^2.8.0",
    "flow-bin": "^0.37.4",
    "flow-copy-source": "^1.1.0",
    "jsdom": "^9.9.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.1.1",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.10.2",
    "tape": "^4.6.3",
    "tcomb": "^3.2.16",
    "tcomb-react": "^0.9.3"
  },
  "scripts": {
    "build": "babel src --out-dir ./lib --ignore **/__tests__/",
    "build:watch": "babel src --watch --out-dir ./lib --ignore **/__tests__/",
    "clean": "rimraf ./lib",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "sass-lint 'src/**/*.scss' -v -q",
    "lint:js": "eslint src cmrh.conf.js .eslintrc.js --ignore-pattern '!.eslintrc.js'",
    "lint:js:fix": "eslint src cmrh.conf.js --ignore-pattern '!.eslintrc.js' --fix",
    "postbuild": "npm run sass && flow-copy-source --ignore *__tests__/* -v src lib",
    "posttest": "npm run lint && npm run flow",
    "prebuild": "npm run clean",
    "prepublish": "npm run test && npm run build",
    "sass": "mkdirp lib/sass && cp src/sass/*.scss lib/sass",
    "test": "tape -r babel-register \"src/**/__tests__/index.js\""
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14 || ^15.0",
    "react-dom": "^0.14 || ^15.0"
  }
}
